{
	"title":"Wave Stats [EU]",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"fontSize=9&areaMode=none&lineWidth=1&yMin=0",
	"dataTemplates":[
	{
		"title":"${explode} - Wave Changes + Images generated",
		"target": [
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.admedia_written, 0.03333), \"Admedia updated/s\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.admedia_retired, 0.03333), \"Admedia retired/s\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.download_failures, 0.03333), \"Bad downloads\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.images_invalid, 0.03333), \"Invalid images/s\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.images_generated, 0.03333), \"Images generated/s\"))"
		],
		"query":"eu-*.cacti_wave.cacti_wave_stats.*",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"${explode} - Wave Changes + Images generated",
		"target": [
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.images_generated, 0.003333), \"Images generated/s\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.admedia_retired, 1.3333), \"Admedia Retired/100s\"))"
		],
		"query":"eu-*.cacti_wave.cacti_wave_stats.*",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"${explode} - Wave Errors",
		"target": [
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.download_failures, 0.003333), \"Download failures/s\"))",
			"cactiStyle(alias(scale(${explode}.cacti_wave.cacti_wave_stats.images_invalid, 0.003333), \"Images invalid/s\"))"
		],
		"query":"eu-*.cacti_wave.cacti_wave_stats.*",
		"regex":"^(.*)",
		"index":0
	}
	],
	"data":[
	]
}
