{
	"title":"DHT Data sent/received [AP]",
	"width":400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":141,
	"refresh":true,
	"legend":true,
	"defaultParameters":"yMin=0&fontSize=9&areaMode=All&areaAlpha=0.4&colorList=green,red",
	"dataTemplates":[
	{
		"title":"${explode} DHT bytes/s sent/received [Node 1]",
		"target": [
			"alias(${explode}.dht-1.dht_stats.bytes_sent, \"bytes sent\")",
			"alias(${explode}.dht-1.dht_stats.bytes_received, \"bytes received\")"
		],
		"query":"ap-*.dht-1.dht_stats",
		"regex":"^(.*)",
		"index":0
	},
