{
    "title":"DHT Logfiles",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"defaultParameters":"areaMode=none&lineWidth=2&yMin=0&fontSize=11&until=-90seconds",
	"data":[
	{
	    "title":"Aggregate - Logfiles Handled Records",
	    "target": "alias(sum(${continent}-*.cacti_dht_log-*.cacti_dht_log.records_handled), \"records handled\")"
	},
	{
	    "title":"Aggregate - Logfiles bytes/s sent/received",
	    "target": [
		"alias(sum(${continent}-*.cacti_dht_log-*.cacti_dht_log.bytes_sent), \"bytes sent\")",
		"alias(sum(${continent}-*.cacti_dht_log-*.cacti_dht_log.bytes_received), \"bytes received\")"
		]
	},
	{
	    "title":"Aggregate - Logfiles Handled Connections",
	    "target": "alias(sum(${continent}-*.cacti_dht_log-*.cacti_dht_log.handling_conns), \"connections handled\")"
	}
	],
	"dataConditional":[
	{
		"title":"LogFiles Total Disk Usage",
		"target":"alias(sum(${explode}.cacti_df-srv.cacti_df.DISKFREE_used), \"total disk space used\")",
		"params":"yMin=0",
		"query":"${server}.cacti_dht_log-*",
		"index":0
	}
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*",
				 "cont-nice" : "All" },
			"EU" : {"continent" : "eu",
				 "cont-nice" : "EU" },
			"US" : {"continent" : "us",
				 "cont-nice" : "US" },
			"AP" : {"continent" : "ap",
				 "cont-nice" : "AP" },
			"USAP" : {"continent" : "usap",
				 "cont-nice" : "USAP" }
		}
	}			
}
