{
	"title":"DHT Handled Connections",
	"width":490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":220,
	"refresh":true,
	"legend":true,
	"defaultParameters":"yMinLeft=0&yMaxLeft=100&&yMinRight=0&fontSize=9&areaMode=first&areaAlpha=0.4&colorList=blue,darkred",
	"dataTemplates":[
	{
		"title":"${explode}.dht-1 DHT Handled Connections",
		"target": [
			"alias(${explode}.dht-1.dht_stats.handling_connections_pcnt, \"% of maximum connections\")",
			"alias(secondYAxis(${explode}.dht-1.dht_stats.handled_connections), \"number of connections\")"
		],
		"query":"${server}.dht-1.dht_stats",
		"index":0
	},
	{
		"title":"${explode}.dht-2 DHT Handled Connections",
		"target": [
			"alias(${explode}.dht-2.dht_stats.handling_connections_pcnt, \"% of maximum connections\")",
			"alias(secondYAxis(${explode}.dht-2.dht_stats.handled_connections), \"number of connections\")"
		],
		"query":"${server}.dht-2.dht_stats",
		"index":0
	},
	{
		"title":"${explode}.dht-3 DHT Handled Connections",
		"target": [
			"alias(${explode}.dht-3.dht_stats.handling_connections_pcnt, \"% of maximum connections\")",
			"alias(secondYAxis(${explode}.dht-3.dht_stats.handled_connections), \"number of connections\")"
		],
		"query":"${server}.dht-3.dht_stats",
		"index":0
	},
	{
		"title":"${explode}.dht-4 DHT Handled Connections",
		"target": [
			"alias(${explode}.dht-4.dht_stats.handling_connections_pcnt, \"% of maximum connections\")",
			"alias(secondYAxis(${explode}.dht-4.dht_stats.handled_connections), \"number of connections\")"
		],
		"query":"${server}.dht-4.dht_stats",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*" },
			"EU":{"continent":"eu" },
			"US":{"continent":"us" },
			"AP":{"continent":"ap" },
			"USAP":{"continent":"usap" }
		},
		"server":{
			"type":"dynamic",
			"query":"${continent}-*.dht-1.dht_stats",
			"index":0,
			"showAll": true,
			"showAllValue":"${continent}-*"
		}
	}
}
