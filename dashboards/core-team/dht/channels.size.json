{
	"title":"DHT Channels size",
	"width":490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":220,
	"columns":2,
	"refresh":true,
	"legend":true,
	"defaultParameters":"yMin=0&fontSize=10&hideLegend=true",
	"dataTemplates":[
	{
		"title":"[${explode} - ${channel}] DHT channel bytes",
		"target":"aliasSub(aliasByNode(${explode}.dht-*.dht_channel-${channel}.bytes, 1, 2), \"dht_channel-\", \"\")",
		"query":"${server}.dht-*.dht_channel-${channel}",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"[${explode} - ${channel}] DHT channel records",
		"target":"aliasSub(aliasByNode(${explode}.dht-*.dht_channel-${channel}.records, 1, 2), \"dht_channel-\", \"\")",
        "query":"${server}.dht-*.dht_channel-${channel}",
		"regex":"^(.*)",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*"},
			"EU":{"continent":"eu"},
			"US":{"continent":"us"},
			"AP":{"continent":"ap"},
			"USAP":{"continent":"usap"}
		},
		"server":{
			"type":"dynamic",
			"query":"${continent}-*.dht-1.dht_stats",
			"index":0,
			"regex":"(.*)",
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"channel":{
			"type":"dynamic",
			"query":"${server}.dht-*.dht_channel-*",
			"index":2,
			"regex":"dht_channel-(.*)",
			"showAll": true,
			"showAllValue":"*"
		}
	}
}
