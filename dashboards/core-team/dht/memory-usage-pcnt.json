{
	"title":"DHT Memory Usage %",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"dataTemplates" : [
	{
		"title":"DHT Memory Usage % on ${explode}",
		"target":"aliasByNode(${explode}.dht-*.process_mem_stats.percentage, 1)",
		"query":"${server}.dht-*.process_mem_stats.percentage",
		"index":0,
		"params":"yMin=0&areaMode=stacked&areaAlpha=0.5&colorList=6666FF,3333FF,0000FF,0000CC,000099,000066&yMax=100"
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" },
			"USAP" : {"continent" : "usap" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.dht-*",
			"index" : 0,
			"showAll": true,
			"showAllValue" : "${continent}-*"
		}
	}
}
