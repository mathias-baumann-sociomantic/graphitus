{
	"title":"DHT Memory Usage %",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"dataTemplates" : [
	{
		"title":"DHT Memory Usage % on ${explode}",
		"target":"alias(scale(divideSeries(sumSeries(${explode}.dht-*.process_mem_stats.rss), ${explode}.cacti_memory.cacti_memory.STAT_memtotal), 100), \"% mem usage\")",
		"query":"${server}.dht-*.process_mem_stats.rss",
		"regex":"^(.*)",
		"index":0,
		"params":"yMin=0&areaMode=all&areaAlpha=0.5&yMax=100"
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" },
			"USAP" : {"continent" : "usap" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.dht-*",
			"index" : 0,
			"showAll": true,
			"showAllValue" : "${continent}-*"
		}
	}
}
