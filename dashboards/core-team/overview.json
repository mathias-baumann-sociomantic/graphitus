{
	"title":"Core-Team overview for ${continent}",
	"width":400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":200,
	"columns":3,
	"refresh":true,
	"legend":false,
	"data":[
	{
		"title":"1. DHT Memory Usage % on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.process_mem_stats.percentage, 0, 1)",
		"params":"yMin=0&yMax=100"
	},
	{
		"title":"2. DHT Handled Records on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.dht_stats.handled_records, 0, 1)"
	},
	{
		"title":"3. DHT Handled Connections on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.dht_stats.handled_connections, 0, 1)"
	},
	{
		"title":"5. LogFiles Handled Records on ${LOGFILES-server}",
		"target":"aliasByNode(${LOGFILES-server}.logfiles-*.logfiles_stats.records_handled, 0, 1)"
	},
	{
		"title":"6. LogFiles Handled Connections on ${LOGFILES-server}",
		"target":"aliasByNode(${LOGFILES-server}.logfiles-*.logfiles_stats.connections_handled, 0, 1)"
	},
	{
		"title":"7. Queue Channels Usage % on ${QUEUE-server}",
		"target":"aliasByNode(${QUEUE-server}.queue-*.queue_channel_stats-*.usage, 0, 1)",
		"query":"${continent}-*.queue-*.queue_channel_stats-*",
		"regex":"queue_channel_stats-(.*)",
		"index":2
	},
	{
		"title":"8. Queue Handled Records on ${QUEUE-server}",
		"target":"aliasByNode(${QUEUE-server}.queue-*.queue_node_stats.records_per_sec, 0, 1)"
	},
	{
		"title":"9. Queue Handled Connections on ${QUEUE-server}",
		"target":"aliasByNode(${QUEUE-server}.queue-*.queue_node_stats.handled_connections, 0, 1)"
	},
	{
		"title":"10. DHTSync Queue High-Prio Channel Usage on ${DHTSYNC-server}",
		"target":"aliasByNode(${DHTSYNC-server}.cacti_dht.cacti_dht_sync_stats.user_yield_records, 0, 1)"
	},
	{
		"title":"11. DHTSync Queue Records/s on ${DHTSYNC-server}",
		"target":"aliasByNode(${DHTSYNC-server}.cacti_dht.cacti_dht_sync_stats.records_handled, 0, 1)"
	},
	{
		"title":"12. DHTSync Queue Connections on ${DHTSYNC-server}",
		"target":"aliasByNode(${DHTSYNC-server}.cacti_dht.cacti_dht_sync_stats.connections_handled, 0, 1)"
	}
	],
	"dataConditional":[
	{
		"title":"4. LogFiles Disk Usage % on ${LOGFILES-server}",
		"target":"aliasByNode(asPercent(${explode}.cacti_df-srv.cacti_df.DISKFREE_used,sumSeriesWithWildcards(${explode}.cacti_df-srv.cacti_df.*,3)), 0)",
		"params":"yMin=0&yMax=100",
		"query":"${LOGFILES-server}.logfiles-*",
		"index":0
	}
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*"},
			"EU":{"continent":"eu"},
			"US":{"continent":"us"},
			"AP":{"continent":"ap"},
			"USAP":{"continent":"usap"}
		},
		"DHT-server": {
			"type":"dynamic",
			"query":"${continent}-*.dht-1",
			"index":0,
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"LOGFILES-server": {
			"type":"dynamic",
			"query":"${continent}-*.logfiles-*",
			"index":0,
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"QUEUE-server": {
			"type":"dynamic",
			"query":"${continent}-*.queue-*",
			"index":0,
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"DHTSYNC-server": {
			"type":"dynamic",
			"query":"${continent}-*.cacti_dht.cacti_dht_sync_stats",
			"index":0,
			"showAll": true,
			"showAllValue":"${continent}-*"
		}
	}
}
