{
	"title":"LogFiles Disk Usage %",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"dataTemplates" : [
	{
		"title":"LogFiles disk usage on ${explode}",
		"target":"aliasByNode(${explode}.cacti_df-srv.cacti_df.DISKFREE_used, 0)",
		"params":"yMin=0",
		"query":"${server}.cacti_dht_log-*",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" },
			"USAP" : {"continent" : "usap" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.cacti_dht.cacti_dht_mem*",
			"index" : 0,
			"regex" : "(.*)",
			"showAll": true,
			"showAllValue" : "${continent}-*"
		}
	}
}
