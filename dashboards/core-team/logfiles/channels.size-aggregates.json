{
	"title":"LogFiles Channels size aggregated",
	"width":490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":220,
	"columns":2,
	"refresh":true,
	"legend":true,
	"defaultParameters":"yMin=0&fontSize=10&hideLegend=true&until=-2min",
	"dataTemplates":[
	{
		"title":"[${continent-nice} - ${explode}] DHT channel bytes",
		"target":"aliasSub(aliasByNode(sumSeries(${continent}-*.logfiles-*.logfiles_channel-${explode}.bytes), 2), \"logfiles_channel-.\", \"\")",
		"query":"${continent}-*.logfiles-*.dht_channel-${channel}",
		"regex":"^logfiles_channel-(.*)",
		"index":2
	},
	{
		"title":"[${continent-nice} - ${explode}] DHT channel records",
		"target":"aliasSub(aliasByNode(sumSeries(${continent}-*.logfiles-*.logfiles_channel-${explode}.records), 1, 2), \"logfiles_channel-.\", \"\")",
		"query":"${continent}-*.logfiles-*.logfiles_channel-${channel}",
		"regex":"^logfiles_channel-(.*)",
		"index":2
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*", "continent-nice":"ALL"},
			"EU":{"continent":"eu", "continent-nice":"EU"},
			"US":{"continent":"us", "continent-nice":"US"},
			"AP":{"continent":"ap", "continent-nice":"AP"},
			"USAP":{"continent":"usap", "continent-nice":"US/AP"}
		},
		"channel":{
			"type":"dynamic",
			"query":"${continent}-*.logfiles-*.logfiles_channel-*",
			"index":2,
			"regex":"logfiles_channel-(.*)",
			"showAll": false
		}
	}
}
