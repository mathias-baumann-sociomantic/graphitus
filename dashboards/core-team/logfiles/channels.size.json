{
	"title":"LogFiles Channels Size",
	"width":490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":220,
	"columns":2,
	"refresh":true,
	"legend":true,
	"defaultParameters":"yMin=0&fontSize=10&hideLegend=true",
	"dataTemplates":[
	{
		"title":"[${explode} - ${channel}] DHT channel bytes",
		"target":"aliasSub(aliasByNode(${explode}.logfiles-*.logfiles_channel-${channel}.bytes, 1, 2), \"logfiles_channel-.\", \"\")",
		"query":"${server}.logfiles-*.logfiles_channel-${channel}",
		"params": "yMin=0",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"[${explode} - ${channel}] DHT channel records",
		"target":"aliasSub(aliasByNode(${explode}.logfiles-*.logfiles_channel-${channel}.records, 1, 2), \"logfiles_channel-.\", \"\")",
		"query":"${server}.logfiles-*.logfiles_channel-${channel}",
		"params": "yMin=0",
		"regex":"^(.*)",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*"},
			"EU":{"continent":"eu"},
			"US":{"continent":"us"},
			"AP":{"continent":"ap"},
			"USAP":{"continent":"usap"}
		},
		"server":{
			"type":"dynamic",
			"query" : "${continent}-*.logfiles-*.logfiles_stats.total_bytes",
			"index":0,
			"regex":"(.*)",
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"channel":{
			"type":"dynamic",
			"query":"${server}.logfiles-*.logfiles_channel-*",
			"index":2,
			"regex":"logfiles_channel-(.*)",
			"showAll": false
		}
	}
}
