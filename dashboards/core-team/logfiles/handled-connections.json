{
	"title":"DHT logfiles Handled Connections",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"defaultParameters":"yMin=0&fontSize=10&hideLegend=false",
	"dataTemplates":[
	{
		"title":"${explode} DHT logfiles Handled Connections [Node 1]",
		"target": "alias(${explode}.logfiles-1.logfiles_stats.connections_handled, \"Handled Connections\")",
		"query":"${server}.logfiles-1.logfiles_stats",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"${explode} DHT logfiles Handled Connections [Node 2]",
		"target": "alias(${explode}.logfiles-2.logfiles_stats.connections_handled, \"Handled Connections\")",
		"query":"${server}.logfiles-2.logfiles_stats",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"${explode} DHT logfiles Handled Connections [Node 3]",
		"target": "alias(${explode}.logfiles-3.logfiles_stats.connections_handled, \"Handled Connections\")",
		"query":"${server}.logfiles-3.logfiles_stats",
		"regex":"^(.*)",
		"index":0
	},
	{
		"title":"${explode} DHT logfiles Handled Connections [Node 4]",
		"target": "alias(${explode}.logfiles-4.logfiles_stats.connections_handled, \"Handled Connections\")",
		"query":"${server}.logfiles-4.logfiles_stats",
		"regex":"^(.*)",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*", "cont-nice" : "All" },
			"EU" : {"continent" : "eu", "cont-nice" : "EU" },
			"US" : {"continent" : "us", "cont-nice" : "US" },
			"AP" : {"continent" : "ap", "cont-nice" : "AP" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.logfiles-*.logfiles_stats.*",
			"index" : 0,
			"regex" : "(.*)",
			"showAll": true,
			"showAllValue" : "${continent}-*"
		}
	}
}
