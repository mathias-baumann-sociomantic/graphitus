{
	"title": "Queues Overview",
	"width": 540,
	"defaultLineWidth": 1,
	"timeBack": "2w",
	"height": 280,
	"columns": 2,
	"refresh": true,
	"legend": false,
	"defaultParameters": "yMin=0",
	"data": [
	{
		"title": "Connections for ${queue} Queue on ${server}",
		"target": "aliasByNode(${server}.queue-${queue}.queue_node_stats.handling_connections, 0, 1)",
		"params": "yMin=0"
	},
	{
		"title": "Processed Records/s for ${queue} Queue on ${server}",
		"target": "aliasByNode(scale(${server}.queue-${queue}.queue_node_stats.records_handled,0.03333), 0, 1)",
		"params": "yMin=0"
	},
	{
		"title": "Bytes/s Sent for ${queue} Queue on ${server}",
		"target": "aliasByNode(scale(${server}.queue-${queue}.queue_node_stats.bytes_sent,0.03333), 0, 1)",
		"params": "yMin=0"
	},
	{
		"title": "Bytes/s Received for ${queue} Queue on ${server}",
		"target": "aliasByNode(scale(${server}.queue-${queue}.queue_node_stats.bytes_received,0.03333), 0, 1)",
		"params": "yMin=0"
	}
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" },
			"USAP" : {"continent" : "usap" }
		},
		"server": {
			"type": "dynamic",
			"query": "${continent}-*.queue-*",
			"index": 0,
			"showAll": true,
			"showAllValue": "${continent}-*"
		},
		"queue": {
			"type": "dynamic",
			"query": "${server}.queue-*",
			"index": 1,
			"regex": "queue-(.*)",
			"showAll": true,
			"showAllValue": "*"
		}
	}
}
