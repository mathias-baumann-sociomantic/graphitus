{
	"title":"Propulsor Backend Stats",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"fontSize=9&areaMode=none&lineWidth=1&yMin=0&until=-90seconds",
	"data":[
	{
		"title":"Aggregate - Propulsor Backend Stats [Bidding Requests/s] [${continent}]",
		"target": [
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.requests_total, \"Total Total\"))",
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.requests_above_1ms, \"Total > 1ms\"))",
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.requests_above_10ms, \"Total > 10ms\"))"
		]
	},
	{
		"title":"Aggregate - Propulsor Backend Stats [DHT Requests/s] [${continent}]",
		"target": [
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.dht_total, \"Total Total\"))",
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.dht_timeout, \"Total Timeout\"))",
			"cactiStyle(alias(aggregates.${continent}.propulsor_be_stats.sum.dht_qfull, \"Total Queue Full\"))"
		]
	}
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "all",
				"cont-nice" : "All" },
			"EU" : {"continent" : "eu",
				"cont-nice" : "EU" },
			"US" : {"continent" : "us",
				"cont-nice" : "US" },
			"AP" : {"continent" : "ap",
				"cont-nice" : "AP" },
			"USAP" : {"continent" : "usap",
				"cont-nice" : "USAP" }
		}
	}			
}
