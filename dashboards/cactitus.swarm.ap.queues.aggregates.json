{
	"title":"Swarm Queues [AP]",
	"width":400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":141,
	"columns":2,
	"refresh":true,
	"legend":false,
	"defaultParameters":"fontSize=9&&areaMode=all&areaAlpha=0.4&colorList=red,green",
	"data":[
	],
	"dataTemplates":[
	{
		"title":"AP - Queue [${explode}] Kb/s sent/received",
		"target": [
			"cactiStyle(alias(ap-*.queue-${explode}.queue_node_stats.data_received, \"Received Data\"))",
			"cactiStyle(alias(ap-*.queue-${explode}.queue_node_stats.data_sent, \"Sent Data\"))"
		],
		"query":"ap-*.queue-*",
		"regex":"queue-(.*)",
		"index":1
	},
	{
		"title":"AP - Queue [${explode}] Connections / Records",
		"target": [
			"cactiStyle(alias(ap-*.queue-${explode}.queue_node_stats.handled_connections, \"Handled Connections\"))",
			"cactiStyle(alias(ap-*.queue-${explode}.queue_node_stats.records_per_sec, \"Records per second\"))"
		],
		"query":"ap-*.queue-*",
		"regex":"queue-(.*)",
		"index":1
	}
	]
}
