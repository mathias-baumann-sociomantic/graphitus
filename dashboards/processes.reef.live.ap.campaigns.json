{
    "title":"Reef Live AP Campaign Stats",
    "width" : 490,
    "defaultLineWidth":1,
    "timeBack":"2h",
    "height" : 220,
    "refresh" : false,
    "refreshIntervalSeconds" : 180,
    "legend" : false,
    "data":[
        {
            "height" : 500,
            "title":"Highest Std Dev of ALL cids",
            "target": [
                "aliasSub(aliasByNode(highestMax(stdev(${continent}-${version}.reef.reef_cid-*.loglines,10),4),0,2),\".reef_cid-\",\" \")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.4&areaMode=all&hideLegend=false"
        },
        {
            "height" : 500,
            "title":"Campaign Loglines/s",
            "target": [
                "aliasSub(aliasByNode(scale(${continent}-${version}.reef.reef_cid-${campaign}.loglines,0.03333333),2),\"reef_cid-\",\"\")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.4&areaMode=all"
        },
        {
            "title":"Campaign FailedCids/s",
            "target": [
                "aliasSub(aliasByNode(scale(${continent}-${version}.reef.reef_cid-${campaign}.failedcids,0.03333333),2),\"reef_cid-\",\"\")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.4&areaMode=all&colorList=%2303FF28,%2300B01A,%23008714,%2353925D"
        },
        {
            "title":"Campaign FailedCids %",
            "target": [
                "aliasSub(aliasByNode(asPercent(${continent}-${version}.reef.reef_cid-${campaign}.failedcids,removeBelowValue(${continent}-${version}.reef.reef_cid-${campaign}.loglines,5)),2),\"reef_cid-\",\"\")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.4&areaMode=all&colorList=%2300B01A,%23008714,%2353925D"
        },
        {
            "title":"Campaign Errors/s",
            "target": [
                "aliasSub(aliasByNode(scale(${continent}-${version}.reef.reef_cid-${campaign}.errors,0.03333333),2),\"reef_cid-\",\"\")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.5&areaMode=all&colorList=darkred"
        },
        {
            "title":"Campaign Errors %",
            "target": [
                "aliasSub(aliasByNode(asPercent(${continent}-${version}.reef.reef_cid-${campaign}.errors,${continent}-${version}.reef.reef_cid-${campaign}.loglines),2),\"reef_cid-\",\"\")"
            ],
            "params" : "lineWidth=2&areaAlpha=0.5&areaMode=all&colorList=red"
        }
    ],
    "parameters": {
        "continent": {
            "AP" : {"continent" : "ap" },
            "EU" : {"continent" : "eu" },
            "US" : {"continent" : "us" },
            "All": {"continent" : "*" }
            },
        "version" : {
            "live" : { "version" : "{137,011,010}" },
            "test" : { "version" : "{001,022,013}" },
            "All" : { "version" : "*" }
        },
        "campaign" : {
            "type" : "dynamic",
            "query" : "${continent}-${version}.reef.reef_cid-*",
            "index" : 2,
            "regex" : "reef_cid-(.*)",
            "showAll": false
        }
    }            
}
