{
	"title":"DHT Memory Usage [EU]",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":141,
	"refresh":true,
	"legend":false,
	"defaultParameters":"yMin=0&fontSize=9&areaMode=all&areaAlpha=0.4&colorList=orange,red",
	"dataTemplates" : [
	{
		"title":"DHT Memory Usage on ${explode} [Node 1]",
		"target":[
			"alias(${explode}.dht-1.process_mem_stats.rss,\"bytes ps\")",
			"alias(${explode}.dht-1.dht_stats.total_bytes,\"bytes dht\")"
		],
		"query":"eu-*.dht-1.dht_stats.total_bytes",
		"regex":"^(.*)",
		"index":0
	},
