{
	"title":"Propulsor Backend CPU usage",
		"width" : 500,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 300,
		"refresh" : true,
		"defaultParameters":"areaMode=all&hideLegend=false&colorList=green,red&yMin=0&yMax=100",
		"dataTemplates" : [
		{
			"title":"${explode} - Propulsor Backend Stats [CPU] [01]",
			"target" : [
				"cactiStyle(alias(${explode}.thruster-01.process_cpu_stats.total,\"CPU\"))"
			],
			"query" : "${continent}-*.thruster-01.process_cpu_stats",
			"regex" : "(.*)",
			"index" : 0
		},
		{
			"title":"${explode} - Propulsor Backend Stats [CPU] [02]",
			"target" : [
				"cactiStyle(alias(${explode}.thruster-02.process_cpu_stats.total,\"CPU\"))"
			],
			"query" : "${continent}-*.thruster-02.process_cpu_stats",
			"regex" : "(.*)",
			"index" : 0
		},
		{
			"title":"${explode} - Propulsor Backend Stats [CPU] [03]",
			"target" : [
				"cactiStyle(alias(${explode}.thruster-03.process_cpu_stats.total,\"CPU\"))"
			],
			"query" : "${continent}-*.thruster-03.process_cpu_stats",
			"regex" : "(.*)",
			"index" : 0
		},
		{
			"title":"${explode} - Propulsor Backend Stats [CPU] [04]",
			"target" : [
				"cactiStyle(alias(${explode}.thruster-04.process_cpu_stats.total,\"CPU\"))"
			],
			"query" : "${continent}-*.thruster-04.process_cpu_stats",
			"regex" : "(.*)",
			"index" : 0
		}
		],
		"data":[
		],
		"parameters": {
        	        "continent": {
	                        "All" : {"continent" : "*",
	                                 "cont-nice" : "All" },
	                        "EU" : {"continent" : "eu",
	                                 "cont-nice" : "EU" },
	                        "US" : {"continent" : "us",
	                                 "cont-nice" : "US" },
	                        "AP" : {"continent" : "ap",
	                                 "cont-nice" : "AP" },
	                        "USAP" : {"continent" : "usap",
	                                 "cont-nice" : "USAP" }
			}
		}			
}
