{
    "title":"Jet",
	"width" : 560,
	"height" : 220,
	"defaultLineWidth" : 1,
	"timeBack":"2h",
	"refresh" : true,
	"legend" : false,
	"data":[
	{
	    "title":"${continent} ${number} Jet Requests",
	    "target": "aliasByNode(scale(${continent}-${number}.cacti_jet.cacti_jet_stats*.number_of_requests,0.033333333),0,2)"
	},
	{
		"title":"${continent} ${number} Jet Requests Aggregated",
	    "target": "aliasByNode(scale(sumSeries(${continent}-${number}.cacti_jet.cacti_jet_stats*.number_of_requests),0.033333333),0,2)",
		"params": "until=-90seconds"
	},
	{
	    "title":"${continent} ${number} Jet Errors",
	    "target": "aliasSub(aliasByNode(scale(exclude(${continent}-${number}.cacti_jet.cacti_jet_stats*.*,\"number_of_requests\"),0.033333333),0,2,3),\"cacti_jet_stats-\",\"\")"
	},
	{
	    "title":"${continent} ${number} Jet Errors Aggregated",
	    "target": "aliasByNode(groupByNode(scale(exclude(${continent}-${number}.cacti_jet.cacti_jet_stats*.*,\"number_of_requests\"),0.033333333),3,\"sumSeries\"),0)",
		"params": "until=-90seconds"
	}
	],
	"parameters": {
		"continent": {
			"All": {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" }
			},
		"number" : {
				"type" : "dynamic",
				"query" : "${continent}-*.cacti_jet",
				"index" : 0,
				"regex" : "[^-]*-(.*)",
				"showAll": true

		}
	}
}
