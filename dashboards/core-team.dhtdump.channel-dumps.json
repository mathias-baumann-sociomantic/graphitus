{
	"title" : "Dhtdump",
	"width" : 550,
	"defaultLineWidth" : 1,
	"timeBack" : "6h",
	"height" : 250,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"until=-120seconds",
	"dataConditional" : [
	{
		"title" : "[${cont-nice} | ${server}] Bytes Written (aggregated)",
		"target" : "aliasByNode(sumSeries(exclude(${explode}.dht-*.dht_dump-*.bytes_written, \"total\")), 0)",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	},
	{
		"title" : "[${cont-nice} | ${server}] Bytes Written (per channel)",
		"target" : "aliasSub(groupByNode(exclude(${explode}.dht-*.dht_dump-*.bytes_written, \"total\"), 2, \"sumSeries\"), \"dht_dump-\", \"\")",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	},
	{
		"title" : "[${cont-nice} | ${server}] Records Written (aggregated)",
		"target" : "aliasByNode(sumSeries(exclude(${server}.dht-*.dht_dump-*.records_written, \"total\")), 0)",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	},
	{
		"title" : "[${cont-nice} | ${server}] Records Written (per channel)",
		"target" : "aliasSub(groupByNode(exclude(${server}.dht-*.dht_dump-*.records_written, \"total\"), 2, \"sumSeries\"), \"dht_dump-\", \"\")",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	},
	{
		"title" : "[${cont-nice} | ${server}] Total Bytes Written Per Second",
		"target" : "aliasByNode(scale(${server}.dht-*.dht_dump-total.bytes_written, 0.033333333),1)",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	},
	{
		"title" : "[${cont-nice} | ${server}] Total Records Written Per Second",
		"target" : "aliasByNode(scale(${server}.dht-*.dht_dump-total.records_written, 0.033333333),1)",
		"query" : "${server}.dht-1.dht_dump-total",
		"regex" : "^(.*)",
		"index" : 0
	}
	],
	"data":[
	],
	"parameters" : {
		"continent" : {
			"All" : { "continent" : "*", "cont-nice" : "All" },
			"EU" : { "continent" : "eu", "cont-nice" : "EU" },
			"US" : { "continent" : "us", "cont-nice" : "US" },
			"AP" : { "continent" : "ap", "cont-nice" : "AP" },
			"USAP" : { "continent" : "usap", "cont-nice" : "USAP" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.dht-*.dht_dump*",
			"index" : 0,
			"regex" : "(.*)",
			"showAll": true,
			"showAllValue" : "${continent}-*"
		}
	}
}
