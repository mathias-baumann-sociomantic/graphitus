{
	"title":"DHT Memory Usage",
	"width":490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":220,
	"refresh":true,
	"legend":true,
	"dataTemplates":[
	{
		"title":"DHT Memory Usage on ${explode} [Node 1]",
		"target":[
			"alias(${explode}.dht-1.dht_stats.total_bytes, \"bytes dht\")",
			"alias(${explode}.dht-1.process_mem_stats.rss, \"bytes ps\")"
		],
		"query":"${server}.dht-1.process_mem_stats.rss",
		"regex":"^(.*)",
		"index":0,
		"params":"yMin=0&areaMode=All&areaAlpha=0.4"
	},

	{
		"title":"DHT Memory Usage on ${explode} [Node 2]",
		"target":[
			"alias(${explode}.dht-2.dht_stats.total_bytes, \"bytes dht\")",
			"alias(${explode}.dht-2.process_mem_stats.rss, \"bytes ps\")"
		],
		"query":"${server}.dht-2.process_mem_stats.rss",
		"regex":"^(.*)",
		"index":0,
		"params":"yMin=0&areaMode=All&areaAlpha=0.4"
	},
	{
		"title":"DHT Memory Usage on ${explode} [Node 3]",
		"target":[
			"alias(${explode}.dht-3.dht_stats.total_bytes, \"bytes dht\")",
			"alias(${explode}.dht-3.process_mem_stats.rss, \"bytes ps\")"
		],
		"query":"${server}.dht-3.process_mem_stats.rss",
		"regex":"^(.*)",
		"index":0,
		"params":"yMin=0&areaMode=All&areaAlpha=0.4"
	},
	{
		"title":"DHT Memory Usage on ${explode} [Node 4]",
		"target":[
			"alias(${explode}.dht-4.dht_stats.total_bytes, \"bytes dht\")",
			"alias(${explode}.dht-4.process_mem_stats.rss, \"bytes ps\")"
		],
		"query":"${server}.dht-4.process_mem_stats.rss",
		"regex":"^(.*)",
		"index":0,
		"params":"yMin=0&areaMode=All&areaAlpha=0.4"
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*" },
			"EU":{"continent":"eu" },
			"US":{"continent":"us" },
			"AP":{"continent":"ap" },
			"USAP":{"continent":"usap" }
		},
		"server":{
			"type":"dynamic",
			"query":"${continent}-*.dht-*.process_mem_stats.rss",
			"index":0,
			"regex":"(.*)",
			"showAll": true,
			"showAllValue":"${continent}-*"
		}
	}			
}
