{
	"title":"Backend Propulsor Aggregates",
		"width" : 490,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 220,
		"columns" : 2,
		"refresh" : true,
		"legend" : true,
		"defaultParameters":"areaMode=none&lineWidth=1&yMin=0&fontSize=11",
		"data":[
		{
			"title":"Aggregate - Propulsor Backend Stats [Bidding Requests/s] [${continent}]",
			"params":"logBase=10&yMin=1001&yMax=100000&minorY=10",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_total), \"Total\")",
			"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_above_1ms), \"> 1 ms\")", 
			"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_above_10ms), \"> 10 ms\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [DHT Requests/s] [${continent}]",
			"params":"logBase=10&yMin=1&yMax=1000000&minorY=10",
			"target": [
				"alias(offset(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_total), 0.1), \"Total\")",
			"alias(offset(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_timeout), 0.1), \"Timeout\")", 
			"alias(offset(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_qfull), 0.1), \"Queue Full\")"
				]
		}
	],
		"data_discarded":[
		{
			"title":"Aggregate - Propulsor Backend Stats [Bidding Requests/s] [${continent}]",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_total), \"Total\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [Bidding Requests/s] [${continent}]",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_above_1ms), \"> 1 ms\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [Bidding Requests/s] [${continent}]",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.requests_above_10ms), \"> 10 ms\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [DHT Requests/s] [${continent}]",
			"params":"colorList=%23B34BFD",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_total), \"Total\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [DHT Requests/s] [${continent}]",
			"params":"colorList=%23B34BFD",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_timeout), \"Timeout\")"
				]
		},
		{
			"title":"Aggregate - Propulsor Backend Stats [DHT Requests/s] [${continent}]",
			"params":"colorList=%23B34BFD",
			"target": [
				"alias(sum(${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-*.dht_qfull), \"Queue Full\")"
