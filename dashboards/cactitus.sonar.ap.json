{
	"title":"Sonar Stats AP",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"fontSize=9&areaMode=none&lineWidth=1&yMin=0",
	"dataTemplates" : [
	{
		"title":"${explode} - Sonar - Nginx Requests/sec [${cont-nice}]",
		"target" : [
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_sockets.nginx_accepts), \"Total Accepts\"))",
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_sockets.nginx_handled), \"Total Handled\"))",
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_sockets.nginx_requests), \"Total Requests\"))"
		],
		"query" : "${continent}.cacti_nginx.cacti_nginx_sockets",
		"regex" : "(.*)",
		"index" : 0
	},
	{
		"title":"${explode} - Sonar - Nginx Clients [${cont-nice}]",
		"target" : [
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_clients.nginx_active), \"Total Active\"))",
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_clients.nginx_reading), \"Total Reading\"))",
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_clients.nginx_waiting), \"Total Waiting\"))",
			"cactiStyle(alias(sum(${explode}.cacti_nginx.cacti_nginx_clients.nginx_writing), \"Total Writing\"))"
		],
		"query" : "${continent}.cacti_nginx.cacti_nginx_clients",
		"regex" : "(.*)",
		"index" : 0
	}
	],
	"data":[
	],
	"parameters": {
                "continent": {
                        "AP" : {"continent" : "{ap-021,ap-022,ap-023,ap-024}",
				"cont-nice" : "AP" },
                        "EU" : {"continent" : "{eu-032,eu-033,eu-034,eu-035,eu-036,eu-053,eu-054,eu-055,eu-056,eu-057,eu-065,eu-085,eu-086,eu-087,eu-088,eu-089,eu-090,eu-091,eu-092}",
				"cont-nice" : "EU" },
                        "US" : {"continent" : "{us-008,us-009,us-019,us-020}",
				"cont-nice" : "US" },
                        "All" : {"continent" : "{eu-032,eu-033,eu-034,eu-035,eu-036,eu-053,eu-054,eu-055,eu-056,eu-057,eu-065,eu-085,eu-086,eu-087,eu-088,eu-089,eu-090,eu-091,eu-092,us-008,us-009,us-019,us-020,ap-021,ap-022,ap-023,ap-024}",
				"cont-nice" : "ALL" }
                }
	}			
}
