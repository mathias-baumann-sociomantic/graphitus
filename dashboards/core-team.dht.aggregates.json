{
	"title":"DHT Memory",
	"width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 220,
	"refresh" : true,
	"legend" : true,
	"defaultParameters":"areaMode=none&lineWidth=2&yMin=0&fontSize=11&until=-90sec",
	"dataConditional" : [
	{
		"title":"DHT Memory Usage % on all nodes from ${continent}",
		"target":"alias(scale(divideSeries(sumSeries(${explode}.dht-*.process_mem_stats.rss), sumSeries(${explode}.cacti_memory.cacti_memory.STAT_memtotal)), 100), \"% mem usage\")",
		"query":"${continent}-*.dht-*.process_mem_stats.rss",
		"regex":"(.*)",
		"index":0,
		"params":"yMin=0&areaMode=none&yMax=100"
	}
	],
	"data":[
	{
		"title":"Aggregate - DHT Handled Connections",
		"target": [
			"alias(sum(${continent}-*.dht-*.dht_stats.handled_connections), \"connections\")"
		]
	},
	{
		"title":"Aggregate - DHT bytes/s sent/received",
		"target": [
			"alias(sum(${continent}-*.dht-*.dht_stats.bytes_sent), \"bytes sent\")",
			"alias(sum(${continent}-*.dht-*.dht_stats.bytes_received), \"bytes received\")"
		]
	},
	{
		"title":"Aggregate - DHT Memory Usage",
		"target": [
			"alias(sum(${continent}-*.dht-*.process_mem_stats.rss), \"bytes ps\")",
			"alias(sum(${continent}-*.dht-*.dht_stats.total_bytes), \"bytes dht\")"
		]

	}
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*" },
			"EU" : {"continent" : "eu" },
			"US" : {"continent" : "us" },
			"AP" : {"continent" : "ap" },
			"USAP" : {"continent" : "usap" }
		}
	}			
}
