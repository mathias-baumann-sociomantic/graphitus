{
	"title":"Propulsor FE Aggregated Stats",
		"width" : 550,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 250,
		"refresh" : true,
		"legend" : true,
		"defaultParameters":"until=-90seconds",
		"data":[
		{
			"title": "Frontend Stats [Backend Requests/s] [${cont-nice}]",
			"target" : "cactiStyle(sortByMinima(aliasByNode(groupByNode(offset(scale(${continent}-${server}.cacti_propulsor-${instance}.cacti_propulsor_fe.*,0.033333333),0.0001),3,\"sumSeries\"),0)))",
			"params" : "logBase=10&yMin=1&yMax=1000000&colorList=red,blue&areaMode=all" 
		},
		{
			"title": "Frontend Stats [Connections] [${cont-nice}]",
			"target" : "cactiStyle(aliasByNode(groupByNode(${continent}-${server}.cacti_propulsor.cacti_propulsor_stats-${instance}.connections*,3,\"sumSeries\"),0))"
		},
		{
			"title": "Frontend Stats [DHT Requests/s] [${cont-nice}]",
			"target" : "cactiStyle(aliasByNode(groupByNode(offset(scale(${continent}-${server}.cacti_propulsor.cacti_propulsor_stats-${instance}.dht*,0.033333333),0.0001),3,\"sumSeries\"),0))",
			"params" : "logBase=10&yMax=1000000"
		},
		{
			"title": "Frontend Stats [Bidding Requests/s] [${cont-nice}]",
			"target" : "aliasSub(aliasByNode(groupByNode(offset(scale(${continent}-${server}.cacti_propulsor-${instance}.cacti_propulsor_fe_stats-${ssp}.requests,0.033333333),0.0001),2,\"sumSeries\"),0),\"^.*-\", \"\")",
			"params" : "logBase=10&yMax=1000000&hideLegend=true"
		},
		{
			"title": "Frontend Stats [Bids/s] [${cont-nice}]",
			"target" : "aliasSub(aliasByNode(groupByNode(offset(scale(${continent}-${server}.cacti_propulsor-${instance}.cacti_propulsor_fe_stats-${ssp}.bids,0.033333333),0.0001),2,\"sumSeries\"),0),\"^.*-\", \"\")",
			"params" : "logBase=10&yMax=1000000&hideLegend=true"
		}
	],
		"parameters": {
			"continent": {
				"All" : {"continent" : "*",
					 "cont-nice" : "All" },
				"EU" : {"continent" : "eu",
					 "cont-nice" : "EU" },
				"US" : {"continent" : "us",
					 "cont-nice" : "US" },
				"AP" : {"continent" : "ap",
					 "cont-nice" : "AP" },
				"USAP" : {"continent" : "usap",
					 "cont-nice" : "USAP" }
			},
			"server" : {
				"type" : "dynamic",
				"query" : "${continent}-*.cacti_propulsor-*",
				"index" : 0,
				"regex" : "-(.*)",
				"showAll": true,
				"defaultValue" : "*"
			},
			"instance" : {
				"type" : "dynamic",
				"query" : "${continent}-${server}.cacti_propulsor-*",
				"index" : 1,
				"regex" : "cacti_propulsor-(.*)",
				"showAll": true,
				"defaultValue" : "*"
			},
			"ssp" : {
				"type" : "dynamic",
				"query" : "${continent}-${server}.cacti_propulsor-*.cacti_propulsor_fe_stats-*",
				"index" : 2,
				"regex" : "-(.*)",
				"showAll": true
			}

		}			
}
