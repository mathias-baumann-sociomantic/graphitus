{
	"title":"Aqua stats",
		"width" : 550,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 250,
		"columns" : 2,
		"refresh" : true,
		"legend" : true,
                "dataTemplates" : [
                {
                        "title":"SSP  Requests/sec",
                        "target2" : "aliasSub(aliasByNode(${continent}-${explode}.cacti_aqua*.cacti_aqua_ssp-*.total_processed,0,3),\"^.*-\", \"\")",
                        "target" : "aliasSub(aliasByNode(groupByNode(scale(timeShift(${continent}-${explode}.cacti_aqua*.cacti_aqua_ssp-*.total_processed, \"60s\"),0.033333333),2,\"sumSeries\"),0),\"^.*-\", \"\")",
                        "query" : "${continent}-*.cacti_aqua*.cacti_aqua_ssp-*.total_processed",
                        "regex" : "-(.*)",
                        "index" : 0,
                        "params" : "yMin=0&hideLegend=true"
                }
         ],

		"data":[
                {
                        "title": "Total requests/s",
                        "target" : [
                                "alias(aliasByNode(groupByNode(scale(timeShift(${continent}-${server}.cacti_aqua*.cacti_aqua.total_queue_handled, \"60s\"),0.033333333),2,\"sumSeries\"),0),\"Total\")",
                                "aliasByNode(scale(timeShift(${continent}-${server}.cacti_aqua*.cacti_aqua.total_queue_handled, \"60s\"),0.033333333),0)"
                        ],
                        "params" : "hideLegend=true"
                },
		{
			"title": "SSP Requests/s",
			"target" : [
				"aliasSub(aliasByNode(groupByNode(scale(timeShift(${continent}-${server}.cacti_aqua*.cacti_aqua_ssp-${ssp}.total_processed, \"60s\"),0.033333333),2,\"sumSeries\"),0),\"^.*-\", \"\")"
			],
			"params" : "hideLegend=true"
		}
        ],
        "parameters": {
                "continent": {
                        "EU" : {"continent" : "eu",
                                 "cont-nice" : "EU" },
                        "US" : {"continent" : "us",
                                 "cont-nice" : "US" },
                        "AP" : {"continent" : "ap",
                                 "cont-nice" : "AP" },
                        "All" : {"continent" : "*",
                                 "cont-nice" : "All" }
                },
                "server" : {
                        "type" : "dynamic",
                        "query" : "${continent}-*.cacti_aqua-*",
                        "index" : 0,
                        "regex" : "-(.*)",
                        "showAll": true,
                        "defaultValue" : "*"
                },
                "ssp" : {
                        "type" : "dynamic",
                        "query" : "${continent}-${server}.cacti_aqua-*.cacti_aqua_ssp-*",
                        "index" : 2,
                        "regex" : "-(.+)",
                        "showAll": true
                }
        }
}
