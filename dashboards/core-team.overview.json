{
	"title":"Core-Team overview for ${continent}",
	"width":400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height":200,
	"columns":3,
	"refresh":true,
	"legend":false,
	"data":[
	{
		"title":"1. DHT Memory Usage % on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.process_mem_stats.percentage, 0, 1)",
		"params":"yMin=0&yMax=100"
	},
	{
		"title":"2. DHT Handled Records on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.dht_stats.handled_records, 0, 1)"
	},
	{
		"title":"3. DHT Handled Connections on ${DHT-server}",
		"target":"aliasByNode(${DHT-server}.dht-*.dht_stats.handled_connections, 0, 1)"
	},
	{
		"title":"5. LogFiles Handled Records on ${LOGFILES-server}",
		"target":"aliasByNode(${LOGFILES-server}.cacti_dht_log-*.cacti_dht_log.records_handled, 0, 1)"
	},
	{
		"title":"6. LogFiles Handled Connections on ${LOGFILES-server}",
		"target":"aliasByNode(${LOGFILES-server}.cacti_dht_log-*.cacti_dht_log.handling_conns, 0, 1)"
	}
	],
	"dataConditional":[
	{
		"title":"4. LogFiles Disk Usage % on ${LOGFILES-server}",
		"target":"aliasByNode(asPercent(${explode}.cacti_df-srv.cacti_df.DISKFREE_used,sumSeriesWithWildcards(${explode}.cacti_df-srv.cacti_df.*,3)), 0)",
		"params":"yMin=0&yMax=100",
		"query":"${LOGFILES-server}.cacti_dht_log-*",
		"index":0
	}
	],
	"parameters": {
		"continent": {
			"All":{"continent":"*"},
			"EU":{"continent":"eu"},
			"US":{"continent":"us"},
			"AP":{"continent":"ap"},
			"USAP":{"continent":"usap"}
		},
		"DHT-server": {
			"type":"dynamic",
			"query":"${continent}-*.dht-1",
			"index":0,
			"regex":"(.*)",
			"showAll": true,
			"showAllValue":"${continent}-*"
		},
		"LOGFILES-server": {
			"type":"dynamic",
			"query":"${continent}-*.cacti_dht_log*",
			"index":0,
			"regex":"(.*)",
			"showAll": true,
			"showAllValue":"${continent}-*"
		}
	}
}
