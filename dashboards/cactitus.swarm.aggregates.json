{
    "title":"Swarm Aggregates",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"areaMode=none&lineWidth=1&yMin=0&fontSize=9&until=-90seconds",
	"data":[
	{
	    "title":"Aggregate - DHT Handled Connections [Total]",
	    "target": [
		"alias(sumSeries(*-*.dht-*.dht_stats.handled_connections), \"connections\")"
		]

	},
	{
	    "title":"Aggregate - DHT bytes/s sent/received [Total]",
	    "target": [
		"alias(sumSeries(*-*.dht-*.dht_stats.bytes_sent), \"bytes sent\")",
		"alias(sumSeries(*-*.dht-*.dht_stats.bytes_received), \"bytes received\")"
		]
	},
	{
	    "title":"Aggregate - DHT Memory Usage [Total]",
	    "target": [
		"alias(sumSeries(*-*.dht-*.process_mem_stats.rss), \"bytes ps\")",
		"alias(sumSeries(*-*.dht-*.dht_stats.total_bytes), \"bytes dht\")"
		]
	}
	]
}


