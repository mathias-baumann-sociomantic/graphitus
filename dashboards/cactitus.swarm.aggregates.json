{
    "title":"Swarm Aggregates",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"areaMode=none&lineWidth=1&yMin=0&fontSize=9&until=-90seconds",
	"data":[
	{
	    "title":"Aggregate - DHT Handled Connections [Total]",
	    "target": [
		"alias(sum(*-*.cacti_dht*.cacti_dht_stats.DHT_handle_connects), \"connections\")"
		]

	},
	{
	    "title":"Aggregate - DHT bytes/s sent/received [Total]",
	    "target": [
		"alias(sum(*-*.cacti_dht*.cacti_dht_stats.DHT_sent_bytes), \"bytes sent\")",
		"alias(sum(*-*.cacti_dht*.cacti_dht_stats.DHT_received_bytes), \"bytes received\")"
		]
	},
	{
	    "title":"Aggregate - DHT Memory Usage [Total]",
	    "target": [
		"alias(sum(*-*.cacti_dht-*.cacti_dht_mem.bytes_ps,*-*.cacti_dht.cacti_dht_mem_ps.bytes_ps_*), \"bytes ps\")",
		"alias(sum(*-*.cacti_dht-*.cacti_dht_mem.bytes_dht,*-*.cacti_dht.cacti_dht_mem_dht.bytes_dht_*), \"bytes dht\")"
		]
	}
