        },
        {
            "title":"SQL Updates/s",
            "target": [
		"aliasByNode(scale(${continent}-*.cacti_cliff.cacti_cliff_stats.sql_updates,0.033333),0,3)"
		],
		"params" : "lineWidth=2"
        },
        {
            "title":"SQL Delay Avg",
            "target": [
		"aliasByNode(${continent}-*.cacti_cliff.cacti_cliff_stats.sql_delay_*,0,3)"
