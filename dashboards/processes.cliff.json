	},
	{
		"title" : "Bid Req. Aggr. Map",
		"params" : "lineWidth=2",
		"target" : "aliasByNode(${continent}-*.cacti_cliff.cacti_cliff_stats.bid_request_map,0,3)"
	},
	{
		"title" : "SQL Queue",
		"target" : "aliasByNode(${continent}-*.cacti_cliff.cacti_cliff_stats.sql_queue,0,3)",
		"params" : "yMin=0&lineWidth=2" 
	},
	{
		"title" : "Memory Usage",
		"params" : "areaAlpha=0.4&areaMode=all&yMin=0",
		"target" : "aliasByNode(sortByMinima(${continent}-*.cacti_cliff.cacti_cliff_stats.memory),0,3)"
