{
    "title":"Cliff Stats",
        "width" : 490,
	"defaultLineWidth":1,
	"timeBack":"6h",
        "height" : 220,
        "columns" : 2,
	"refresh" : true,
        "legend" : false,
        "data":[
        {
            "title":"Bids",
            "target": [
		"cactiStyle(aliasByNode(scale(sortByMinima(${continent}-*.cacti_cliff.cacti_cliff_stats.bids),0.03333),0))"
		],
		"params" : "yMin=0&areaAlpha=0.5&areaMode=all&lineWidth=2&colorList=%23B34BFD,%23C87CFF,%239D63FF,%235E00FF,%238C00FF,%23B969FB,%23F85F00,%23F8002D,%23F82700"

        },
	{
            "title":"Bid Requests",
            "target": [
		"cactiStyle(aliasByNode(scale(sortByMinima(${continent}-*.cacti_cliff.cacti_cliff_stats.bid_requests),0.03333),0))"
		],
		"params" : "yMin=0&areaAlpha=0.5&areaMode=all&lineWidth=2&colorList=%23FFEF00,%23FFD832,%23D9BD45,%23F7E48E"

        },
	{
            "title":"Bid Request Packets",
            "target": [
		"cactiStyle(aliasByNode(scale(sortByMinima(${continent}-*.cacti_cliff.cacti_cliff_stats.bid_request_packets),0.03333),0))"
