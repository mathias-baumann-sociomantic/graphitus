{
	"title":"Host stats",
	"width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
	"height" : 141,
	"refresh" : true,
	"legend" : false,
	"defaultParameters":"fontSize=9&areaMode=none&lineWidth=1&yMin=0",
	"dataTemplates" : [
	{
		"title":"01. Context Switches [${explode}]",
		"target": [
			"cactiStyle(alias(${explode}.cacti_contextswitch.cacti_contextswitch.STAT_context_swtchs, \"Context Switches\"))"
		],
		"query":"${server}.${metric_filter}.cacti_contextswitch.STAT_context_swtchs",
		"index":0
	},
	{
		"title":"02. CPU Usage [${explode}]",
		"target": [
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_guest, \"CPU Guest\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_steal, \"CPU Steal\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_softirq, \"CPU Softirq\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_irq, \"CPU Irq\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_nice, \"CPU Nice\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_iowait, \"CPU Iowait\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_system, \"CPU System\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_user, \"CPU User\")",
			"alias(${explode}.cacti_cpu.cacti_cpu.STAT_CPU_idle, \"CPU Idle\")"
		],
		"params":"areaMode=stacked&colorList=pink,magenta,grey,aqua,purple,red,brown,orange,green",
		"query":"${server}.${metric_filter}.cacti_cpu.STAT_CPU_guest",
		"index":0
	},
	{
		"title":"03. Disk Elapsed IO Time (ms) [${explode}] [per-sdX]",
		"target": [
			"aliasSub(${explode}.cacti_disk-sd[abcd].cacti_disk_io_time.DISK_io_time, \"^.*cacti_disk-sd(.?)\\..*\", \"sd\\1 Io Time\")",
			"aliasSub(${explode}.cacti_disk-sd[abcd].cacti_disk_io_time.DISK_io_time_weghtd, \"^.*cacti_disk-sd(.?)\\..*\", \"sd\\1 Io Time Weighted\")"
		],
		"query":"${server}.${metric_filter}-sd[abcd].cacti_disk_io_time.DISK_io_time_weghtd",
		"index":0
	},
	{
		"title":"04. Disk Read/Write Time (ms) [${explode}] [per-sdX]",
		"target": [
			"aliasSub(${explode}.cacti_disk-sd[abcd].cacti_disk_rw_time.DISK_time_spnt_rdng, \"^.*cacti_disk-sd(.?)\\..*\", \"sd\\1 Time Spent Reading\")",
			"aliasSub(scale(${explode}.cacti_disk-sd[abcd].cacti_disk_rw_time.DISK_tme_spnt_wrtng, -1), \"^.*cacti_disk-sd(.?)\\..*\", \"sd\\1 Time Spent Writing\")"
		],
		"params":"yMin=",
		"query":"${server}.${metric_filter}-sd[abcd].cacti_disk_rw_time.DISK_time_spnt_rdng",
		"index":0
	},
	{
		"title":"05. Disk Sectors Read/Written [${explode}] [per-mdX]",
		"target": [
			"aliasSub(${explode}.cacti_disk-md*.cacti_disk_sec_rdwt.DISK_sectors_read, \"^.*-md(\\d+).*\", \"md-\\1 Sectors Read\")",
			"aliasSub(scale(${explode}.cacti_disk-md*.cacti_disk_sec_rdwt.DISK_sectors_writtn, -1), \"^.*-md(\\d+).*\",  \"md-\\1 Sectors Written\")"
		],
		"params":"yMin=",
		"query":"${server}.${metric_filter}-md3.cacti_disk_sec_rdwt.DISK_sectors_read",
		"index":0
	},
	{
		"title":"06. Disk Operations [${explode}] [md3]",
		"target": [
			"alias(${explode}.cacti_disk-md3.cacti_disk_ops.DISK_reads, \"Reads\")",
			"alias(${explode}.cacti_disk-md3.cacti_disk_ops.DISK_reads_merged, \"Reads Merged\")",
			"alias(scale(${explode}.cacti_disk-md3.cacti_disk_ops.DISK_writes, -1), \"Writes\")",
			"alias(scale(${explode}.cacti_disk-md3.cacti_disk_ops.DISK_writes_merged, -1), \"Writes Merged\")"
		],
		"params":"areaMode=all&yMin=",
		"query":"${server}.${metric_filter}-md3.cacti_disk_ops.DISK_reads",
		"index":0
	},
	{
		"title":"07. Disk Usage [${explode}] [/]",
		"target": [
			"alias(${explode}.cacti_df-root.cacti_df.DISKFREE_used, \"Used\")",
			"alias(${explode}.cacti_df-root.cacti_df.DISKFREE_available, \"Free\")"
		],
		"params":"areaMode=stacked",
		"query":"${server}.${metric_filter}-root.cacti_df.DISKFREE_used",
		"index":0
	},
	{
		"title":"07. Disk Usage [${explode}] [/srv]",
		"target": [
			"alias(${explode}.cacti_df-srv.cacti_df.DISKFREE_used, \"Used\")",
			"alias(${explode}.cacti_df-srv.cacti_df.DISKFREE_available, \"Free\")"
		],
		"params":"areaMode=stacked",
		"query":"${server}.${metric_filter}-srv.cacti_df.DISKFREE_used",
		"index":0
	},
	{
		"title":"07. Disk Usage [${explode}] [/mnt-static]",
		"target": [
			"alias(${explode}.cacti_df-mnt-static.cacti_df.DISKFREE_used, \"Used\")",
			"alias(${explode}.cacti_df-mnt-static.cacti_df.DISKFREE_available, \"Free\")"
		],
		"params":"areaMode=stacked",
		"query":"${server}.${metric_filter}-mnt-static.cacti_df.DISKFREE_used",
		"index":0
	},
	{
		"title":"08. Forks [${explode}]",
		"target": [
			"alias(${explode}.cacti_forks.cacti_forks.STAT_forks, \"Forks\")"
		],
		"query":"${server}.${metric_filter}.cacti_forks.STAT_forks",
		"index":0
	},
	{
		"title":"09. Interrupts [${explode}]",
		"target": [
			"alias(${explode}.cacti_interrupts.cacti_interrupts.STAT_interrupts, \"Interrupts\")"
		],
		"query":"${server}.${metric_filter}.cacti_interrupts.STAT_interrupts",
		"index":0
	},
	{
		"title":"10. Load Average [${explode}]",
		"target": [
			"alias(${explode}.cacti_load.cacti_load.STAT_loadavg, \"Loadavg\")"
		],
		"query":"${server}.${metric_filter}.cacti_load.STAT_loadavg",
		"index":0
	},
	{
		"title":"11. Memory [${explode}]",
		"target": [
			"alias(sumSeries(${explode}.cacti_memory.cacti_memory.STAT_memused,${explode}.cacti_memory.cacti_memory.STAT_memshared), \"Memused\")",
			"alias(${explode}.cacti_memory.cacti_memory.STAT_memcached, \"Memcached\")",
			"alias(${explode}.cacti_memory.cacti_memory.STAT_membuffer, \"Membuffer\")",
			"alias(${explode}.cacti_memory.cacti_memory.STAT_memfree, \"Memfree\")"
		],
		"params":"areaMode=stacked&colorList=red,orange,brown,green",
		"query":"${server}.${metric_filter}.cacti_memory.STAT_memfree",
		"index":0
	},
	{
		"title":"12. Swap Operations [${explode}]",
		"target": [
			"alias(${explode}.cacti_swap.cacti_swap.VMSTAT_pswpin, \"Swap IN operations\")",
			"alias(${explode}.cacti_swap.cacti_swap.VMSTAT_pswpout, \"Swap OUT operations\")"
		],
		"query":"${server}.${metric_filter}.cacti_swap",
		"index":0
	},
	{
		"title":"13. Network Traffic bytes [${explode}] [eth0]",
		"target": [
			"alias(scale(${explode}.cacti_netdev-eth0.cacti_netdev.NETDEV_rxbytes,-1), \"Received Bytes\")",
			"alias(${explode}.cacti_netdev-eth0.cacti_netdev.NETDEV_txbytes, \"Sent bytes\")"
		],
		"params":"areaMode=all&yMin=",
		"query":"${server}.${metric_filter}-eth0.cacti_netdev.NETDEV_rxbytes",
		"index":0
	},
	{
		"title":"13. Network Traffic bytes [${explode}] [eth1]",
		"target": [
			"alias(scale(${explode}.cacti_netdev-eth1.cacti_netdev.NETDEV_rxbytes,-1), \"Received Bytes\")",
			"alias(${explode}.cacti_netdev-eth1.cacti_netdev.NETDEV_txbytes, \"Sent bytes\")"
		],
		"params":"areaMode=all&yMin=",
		"query":"${server}.${metric_filter}-eth1.cacti_netdev.NETDEV_rxbytes",
		"index":0
	},
	{
		"title":"14. Number of Users [${explode}]",
		"target": [
			"alias(${explode}.cacti_users.cacti_users.STAT_numusers, \"Numusers\")"
		],
		"params":"",
		"query":"${server}.${metric_filter}.cacti_users.STAT_numusers",
		"index":0
	},
	{
		"title":"15. Sockets Pipes Files [${explode}]",
		"target": [
			"alias(${explode}.cacti_sfpstat.cacti_sfpstat.files, \"Files\")",
			"alias(${explode}.cacti_sfpstat.cacti_sfpstat.pipes, \"Pipes\")",
			"alias(${explode}.cacti_sfpstat.cacti_sfpstat.tcp, \"TCP\")",
			"alias(${explode}.cacti_sfpstat.cacti_sfpstat.udp, \"UDP\")",
			"alias(${explode}.cacti_sfpstat.cacti_sfpstat.unix, \"Unix\")"
		],
		"query":"${server}.${metric_filter}.cacti_sfpstat",
		"index":0
	}
	],
	"data":[
	],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*", 
				"continent-nice" : "ALL" },
			"USAP" : {"continent" : "usap",
				"continent-nice" : "US/AP" },
			"EU" : {"continent" : "eu",
				"continent-nice" : "EU" },
			"US" : {"continent" : "us",
				"continent-nice" : "US" },
			"AP" : {"continent" : "ap",
				"continent-nice" : "AP" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*",
			"index" : 0,
			"showAll": true,
			"showAllValue" : "${continent}-*"
		},
		"metric_filter" : {
			"All" : {
				"metric_filter" : "*",
				"metric_filter-nice" : "ALL"
			},
			"cacti_contextswitch" : {
				"metric_filter" : "cacti_contextswitch",
				"metric_filter-nice" : "Context Switches"
			},
			"cacti_cpu" : {
				"metric_filter" : "cacti_cpu",
				"metric_filter-nice" : "CPU"
			},
			"cacti_disk" : {
				"metric_filter" : "cacti_disk",
				"metric_filter-nice" : "Disk Operations"
			},
			"cacti_df" : {
				"metric_filter" : "cacti_df",
				"metric_filter-nice" : "Disk Free"
			},
			"cacti_forks" : {
				"metric_filter" : "cacti_forks",
				"metric_filter-nice" : "Forks"
			},
			"cacti_interrupts" : {
				"metric_filter" : "cacti_interrupts",
				"metric_filter-nice" : "Interrupts"
			},
			"cacti_load" : {
				"metric_filter" : "cacti_load",
				"metric_filter-nice" : "Unixload"
			},
			"cacti_memory" : {
				"metric_filter" : "cacti_memory",
				"metric_filter-nice" : "Memory"
			},
			"cacti_swap" : {
				"metric_filter" : "cacti_swap",
				"metric_filter-nice" : "Swap"
			},
			"cacti_netdev" : {
				"metric_filter" : "cacti_netdev",
				"metric_filter-nice" : "Net Devices"
			},
			"cacti_users" : {
				"metric_filter" : "cacti_users",
				"metric_filter-nice" : "Users"
			},
			"cacti_sfpstat" : {
				"metric_filter" : "cacti_sfpstat",
				"metric_filter-nice" : "SFP stats"
			}
		}
	}
}
