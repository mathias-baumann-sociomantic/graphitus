{
	"title":"Propulsor Backend DHT Requests",
		"width" : 500,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 300,
		"columns" : 2,
		"refresh" : true,
		"defaultParameters":"areaMode=none&lineWidth=1&fontSize=11",
		"dataTemplates" : [
		{
			"title":"${explode} Backend DHT Requests/sec [01]",
			"target" : [
				"aliasByNode(scale(offset(${explode}.cacti_propulsor.cacti_propulsor_be_stats-01.dht_total,0.1),0.03333),3)",
				"aliasByNode(scale(offset(${explode}.cacti_propulsor.cacti_propulsor_be_stats-01.dht_timeout,0.1),0.03333),3)",
				"aliasByNode(scale(offset(${explode}.cacti_propulsor.cacti_propulsor_be_stats-01.dht_qfull,0.1),0.03333),3)"
			],
			"query" : "${continent}-*.cacti_propulsor.cacti_propulsor_be_stats-01",
			"regex" : "(.*)",
			"index" : 0,
			"params" : "hideLegend=false&colorList=black,red,green&logBase=10&yMin=0.1&yMax=10000&minorY=10"
		},
		{
