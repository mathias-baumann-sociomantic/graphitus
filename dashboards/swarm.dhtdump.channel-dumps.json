{
	"title":"Dhtdump",
		"width" : 550,
		"defaultLineWidth":1,
		"timeBack":"6h",
		"height" : 250,
		"columns" : 2,
		"refresh" : true,
		"legend" : true,
		"data":[
			{
				"title": "Bytes Written Per Channel",
				"target" : [
					"sumSeries(exclude(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-*.bytes_written, \"45s\"), \"total\"))",
					"groupByNode(exclude(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-*.bytes_written, \"45s\"), \"total\"), 2, \"sumSeries\")"
				],
				"params" : "hideLegend=true"
			},
			{
				"title": "Records Written Per Channel",
				"target" : [
					"sumSeries(exclude(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-*.records_written, \"45s\"), \"total\"))",
					"groupByNode(exclude(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-*.records_written, \"45s\"), \"total\"), 2, \"sumSeries\")"
				],
				"params" : "hideLegend=true"
			},
			{
				"title": "Bytes Written Per Second",
				"target" : [
					"scale(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-total.bytes_written, \"45s\"), 0.033333333))"
				],
				"params" : "hideLegend=true"
			},
			{
				"title": "Records Written Per Second",
				"target" : [
					"scale(timeShift(${continent}-${server}.cacti_dht-*.dht_dump-total.records_written, \"45s\"), 0.033333333)"
				],
				"params" : "hideLegend=true"
			}		],
	"parameters": {
		"continent": {
			"All" : {"continent" : "*",
				 "cont-nice" : "All" },
			"EU" : {"continent" : "eu",
				 "cont-nice" : "EU" },
			"US" : {"continent" : "us",
				 "cont-nice" : "US" },
			"AP" : {"continent" : "ap",
				 "cont-nice" : "AP" },
			"USAP" : {"continent" : "usap",
				 "cont-nice" : "USAP" }
		},
		"server" : {
			"type" : "dynamic",
			"query" : "${continent}-*.cacti_dht-*.dht_dump*",
			"index" : 0,
			"regex" : "-(.*)",
			"showAll": true,
			"defaultValue" : "*"
		}
	}			
}
