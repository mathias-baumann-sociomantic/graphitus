{
    "title":"Swarm DHT-Logfiles Aggregates [US]",
        "width" : 400,
	"defaultLineWidth":1,
	"timeBack":"6h",
        "height" : 141,
	"refresh" : true,
        "legend" : false,
	"defaultParameters":"areaMode=none&lineWidth=1&yMin=0&fontSize=9",
        "data":[
	{
            "title":"Aggregate - Logfiles Total Bytes [US]",
            "target": "alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.total_bytes), \"total bytes\")"
        },
	{
            "title":"Aggregate - Logfiles Total Records [US]",
            "target": "alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.total_records), \"total records\")"
        },
	{
            "title":"Aggregate - Logfiles bytes/s sent/received [US]",
            "target": [
		"alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.bytes_sent), \"bytes sent\")",
		"alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.bytes_received), \"bytes received\")"
		]
        },
        {
            "title":"Aggregate - Logfiles Handled Records [US]",
            "target": [
		"alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.records_handled), \"records handled\")"
		]
        },
        {
            "title":"Aggregate - Logfiles Handled Connections [US]",
            "target": [
		"alias(sum(us-*.cacti_dht_log-*.cacti_dht_log.handling_conns), \"connections handled\")"
		]
        }
        ]
}
